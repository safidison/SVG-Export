{
    "name": "SVG Export",
    "description": "Download SVGs from websites as PNGs or JPEGs",
    "version": "1.0.5",
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "icons": {
        "16": "icons/16x16.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "browser_action": {
        "default_icon": {
           "19": "icons/19x19.png",
           "38": "icons/38x38.png"
        },
        "default_popup": "popup.html",
        "default_title": "SVG Export"
    },
    "permissions": [
        "activeTab"
    ],
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
